gonghao: [\r\n            { required: true, message: '护工工号不能为空', trigger: 'blur' },\r\n          ],\r\n          hugongxingming: [\r\n            { required: true, message: '护工姓名不能为空', trigger: 'blur' },\r\n          ],\r\n          mima: [\r\n            { required: true, message: '密码不能为空', trigger: 'blur' },\r\n          ],\r\n          xingbie: [\r\n          ],\r\n          nianling: [\r\n            { validator: this.$validate.isIntNumer, trigger: 'blur' },\r\n          ],\r\n          hugongzhaopian: [\r\n          ],\r\n          congyejingyan: [\r\n          ],\r\n          lianxidianhua: [\r\n            { validator: this.$validate.isMobile, trigger: 'blur' },\r\n          ],\r\n          hugongzili: [\r\n          ],\r\n          jiwanghuli: [\r\n          ],\r\n          gerenpingjia: [\r\n          ],\r\n        },\r\n\t\tcenterType: false,\r\n      };\r\n    },\r\n    computed: {\r\n\r\n\r\n\r\n    },\r\n    components: {\r\n    },\r\n    created() {\r\n\t\tif(this.$route.query.centerType){\r\n\t\t\tthis.centerType = true\r\n\t\t}\r\n\t  //this.bg();\r\n      let type = this.$route.query.type ? this.$route.query.type : '';\r\n      this.init(type);\r\n      this.baseUrl = this.$config.baseUrl;\r\n    },\r\n    methods: {\r\n      getMakeZero(s) {\r\n          return s < 10 ? '0' + s : s;\r\n      },\r\n      // 下载\r\n      download(file){\r\n        window.open(`${file}`)\r\n      },\r\n      // 初始化\r\n      init(type) {\r\n        this.type = type;\r\n        if(type=='cross'){\r\n          var obj = JSON.parse(localStorage.getItem('crossObj'));\r\n          for (var o in obj){\r\n            if(o=='hugonggonghao'){\r\n              this.ruleForm.hugonggonghao = obj[o];\r\n              this.ro.hugonggonghao = true;\r\n              continue;\r\n            }\r\n            if(o=='hugongxingming'){\r\n              this.ruleForm.hugongxingming = obj[o];\r\n              this.ro.hugongxingming = true;\r\n              continue;\r\n            }\r\n            if(o=='mima'){\r\n              this.ruleForm.mima = obj[o];\r\n              this.ro.mima = true;\r\n              continue;\r\n            }\r\n            if(o=='xingbie'){\r\n              this.ruleForm.xingbie = obj[o];\r\n              this.ro.xingbie = true;\r\n              continue;\r\n            }\r\n            if(o=='nianling'){\r\n              this.ruleForm.nianling = obj[o];\r\n              this.ro.nianling = true;\r\n              continue;\r\n            }\r\n            if(o=='hugongzhaopian'){\r\n              this.ruleForm.hugongzhaopian = obj[o].split(\",\")[0];\r\n              this.ro.hugongzhaopian = true;\r\n              continue;\r\n            }\r\n            if(o=='congyejingyan'){\r\n              this.ruleForm.congyejingyan = obj[o];\r\n              this.ro.congyejingyan = true;\r\n              continue;\r\n            }\r\n            if(o=='lianxidianhua'){\r\n              this.ruleForm.lianxidianhua = obj[o];\r\n              this.ro.lianxidianhua = true;\r\n              continue;\r\n            }\r\n            if(o=='hugongzili'){\r\n              this.ruleForm.hugongzili = obj[o];\r\n              this.ro.hugongzili = true;\r\n              continue;\r\n            }\r\n            if(o=='jiwanghuli'){\r\n              this.ruleForm.jiwanghuli = obj[o];\r\n              this.ro.jiwanghuli = true;\r\n              continue;\r\n            }\r\n            if(o=='gerenpingjia'){\r\n              this.ruleForm.gerenpingjia = obj[o];\r\n              this.ro.gerenpingjia = true;\r\n              continue;\r\n            }\r\n          }\r\n        }else if(type=='edit'){\r\n\t\t\tthis.info()\r\n\t\t}\r\n        // 获取用户信息\r\n        this.$http.get(this.userTableName + '/session', {emulateJSON: true}).then(res => {\r\n          if (res.data.code == 0) {\r\n            var json = res.data.data;\r\n          }\r\n        });\r\n        this.xingbieOptions = \"男,女\".split(',')\r\n\r\n\t\tif (localStorage.getItem('raffleType'