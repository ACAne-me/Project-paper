    :multiple=\"true\"\r\n            :fileUrls=\"ruleForm.hugongzhaopian?ruleForm.hugongzhaopian:''\"\r\n            @change=\"hugongzhaopianUploadChange\"\r\n            ></file-upload>\r\n          </el-form-item>\r\n            <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' class=\"upload\" v-else label=\"护工照片\" prop=\"hugongzhaopian\">\r\n                <img v-if=\"ruleForm.hugongzhaopian.substring(0,4)=='http'\" class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" :src=\"ruleForm.hugongzhaopian.split(',')[0]\" width=\"100\" height=\"100\">\r\n                <img v-else class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in ruleForm.hugongzhaopian.split(',')\" :src=\"baseUrl+item\" width=\"100\" height=\"100\">\r\n            </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"服务类型\" prop=\"fuwuleixing\">\r\n            <el-select  @change=\"fuwuleixingChange\" v-model=\"ruleForm.fuwuleixing\" placeholder=\"请选择服务类型\">\r\n              <el-option\r\n                  v-for=\"(item,index) in fuwuleixingOptions\"\r\n                  :key=\"index\"\r\n                  :label=\"item\"\r\n                  :value=\"item\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"价格\" prop=\"jiage\">\r\n            <el-input v-model.number=\"ruleForm.jiage\" \r\n                placeholder=\"价格\" clearable :disabled=\" false  ||ro.jiage\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"预约时间\" prop=\"yuyueshijian\">\r\n              <el-date-picker\r\n\t\t\t\t  :disabled=\" false  ||ro.yuyueshijian\"\r\n                  value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n                  v-model=\"ruleForm.yuyueshijian\" \r\n                  type=\"datetime\"\r\n                  placeholder=\"预约时间\">\r\n              </el-date-picker>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"预约备注\" prop=\"yuyuebeizhu\">\r\n            <el-input v-model=\"ruleForm.yuyuebeizhu\" \r\n                placeholder=\"预约备注\" clearable :disabled=\" false  ||ro.yuyuebeizhu\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"用户账号\" prop=\"yonghuzhanghao\">\r\n            <el-input v-model=\"ruleForm.yonghuzhanghao\" \r\n                placeholder=\"用户账号\" clearable :disabled=\"true  ||ro.yonghuzhanghao\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"姓名\" prop=\"xingming\">\r\n            <el-input v-model=\"ruleForm.xingming\" \r\n                placeholder=\"姓名\" clearable :disabled=\"true  ||ro.xingming\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}' label=\"手机\" prop=\"shouji\">\r\n            <el-input v-model=\"ruleForm.shouji\" \r\n                placeholder=\"手机\" clearable :disabled=\"true  ||ro.shouji\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}'  label=\"服务状态\" prop=\"fuwuzhuangtai\">\r\n            <el-select v-model=\"ruleForm.fuwuzhuangtai\" placeholder=\"请选择服务状态\" :disabled=\"true  ||ro.fuwuzhuangtai\" >\r\n              <el-option\r\n                  v-for=\"(item,index) in fuwuzhuangtaiOptions\"\r\n                  :key=\"index\"\r\n                  :label=\"item\"\r\n                  :value=\"item\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n\r\n      <el-form-item :style='{\"padding\":\"0\",\"margin\":\"0\"}'>\r\n        <el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0\",\"margin\":\"0 20px 0 0\",\"outline\":\"none\",\"color\":\"#333\",\"borderRadius\":\"0px\",\"background\":\"#b4ebcc\",\"width\":\"100px\",\"lineHeight\":\"36px\",\"fontSize\":\"14px\",\"height\":\"36px\"}'  type=\"primary\" @click=\"onSubmit\">提交</el-button>\r\n        <el-button :style='{\"border\":\"1px solid #eee\",\"cursor\":\"pointer\",\"padding\":\"0\",\"margin\":\"0\",\"outline\":\"none\",\"color\":\"#333\",\"borderRadius\":\"0px\",\"background\":\"none\",\"width\":\"100px\",\"lineHeight\":\"36px\",\"fontSize\":\"14px\",\"height\":\"36px\"}' @click=\"back()\">返回</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    data() {\r\n\t  let self = this\r\n      return {\r\n        id: '',\r\n        baseUrl: '',\r\n        ro:{\r\n\t\t\t\thugonggonghao : false,\r\n\t\t\t\thugongxingming : false,\r\n\t\t\t\thugongzhaopian : false,\r\n\t\t\t\tfuwuleixing : false,\r\n\t\t\t\tjiage : false,\r\n\t\t\t\tyuyueshijian : false,\r\n\t\t\t\tyuyuebeizhu : false,\r\n\t\t\t\tyonghuzhanghao : false,\r\n\t\t\t\txingming : false,\r\n\t\t\t\tshouji : false,\r\n\t\t\t\tfuwuzhuangtai : false,\r\n\t\t\t\tsfsh : false,\r\n\t\t\t\tshhf : false,\r\n        },\r\n        type: '',\r\n        userTableName: localStorage.getItem('UserTableName'),\r\n        ruleForm: {\r\n          hugonggonghao: '',\r\n          hugongxingming: '',\r\n          hugongzhaopian: '',\r\n          fuwuleixing: '',\r\n          jiage: '',\r\n          yuyueshijian: '',\r\n          yuyuebeizhu: '',\r\n          yonghuzhanghao: '',\r\n          xingming: '',\r\n          shouji: '',\r\n          fuwuzhuangtai: '未服务' ,\r\n        },\r\n        fuwuleixingOptions: [],\r\n        fuwuzhuangtaiOptions: [],\r\n\r\n\r\n        rules: {\r\n          hugonggonghao: [\r\n          ],\r\n          hugongxingming: [\r\n          ],\r\n          hugongzhaopian: [\r\n          ],\r\n          fuwuleixing: [\r\n          ],\r\n          jiage: [\r\n            { validator: this.$validate.isIntNumer, trigger: 'blur' },\r\n          ],\r\n          yuyueshijian: [\r\n          ],\r\n          yuyuebeizhu: [\r\n          ],\r\n          yonghuzhanghao: [\r\n            { required: true, message: '用户账号不能为空', trigger: 'blur' },\r\n          ],\r\n          xingming: [\r\n            { required: true, message: '姓名不能为空', trigger: 'blur' },\r\n          ],\r\n          shouji: [\r\n            { required: true, message: '手机不能为空', trigger: 'blur' },\r\n          ],\r\n          fuwuzhuangtai: [\r\n          ],\r\n          sfsh: [\r\n          ],\r\n          shhf: [\r\n          ],\r\n        },\r\n\t\tcenterType: false,\r\n      };\r\n    },\r\n    computed: {\r\n\r\n\r\n\r\n    },\r\n    components: {\r\n    },\r\n    created() {\r\n\t\tif(this.$route.query.centerType){\r\n\t\t\tthis.centerType = true\r\n\t\t}\r\n\t  //this.bg();\r\n      let type = this.$route.query.type ? this.$route.query.type : '';\r\n      this.init(type);\r\n      this.baseUrl = this.$config.baseUrl;\r\n      this.ruleForm.yuyueshijian = this.getCurDateTime()\r\n    },\r\n    methods: {\r\n      getMakeZero(s) {\r\n          return s < 10 ? '0' + s : s;\r\n      },\r\n      // 下载\r\n      download(file){\r\n        window.open(`${file}`)\r\n      },\r\n      // 初始化\r\n      init(type) {\r\n        this.type = type;\r\n        if(type=='cross'){\r\n          var obj = JSON.parse(localStorage.getItem('crossObj'));\r\n          for (var o in obj){\r\n            if(o=='hugonggonghao'){\r\n              this.ruleForm.hugonggonghao = obj[o];\r\n              this.ro.hugonggonghao = true;\r\n              continue;\r\n            }\r\n            if(o=='hugongxingming'){\r\n              this.ruleForm.hugongxingming = obj[o];\r\n              this.ro.hugongxingming = true;\r\n              continue;\r\n            }\r\n            if(o=='hugongzhaopian'){\r\n              this.ruleForm.hugongzhaopian = obj[o].split(\",\")[0];\r\n              this.ro.hugongzhaopian = true;\r\n              continue;\r\n            }\r\n            if(o=='fuwuleixing'){\r\n              this.ruleForm.fuwuleixing = obj[o];\r\n              this.ro.fuwuleixing = true;\r\n              continue;\r\n            }\r\n            if(o=='jiage'){\r\n              this.ruleForm.jiage = obj[o];\r\n              this.ro.jiage = true;\r\n              continue;\r\n            }\r\n            if(o=='yuyueshijian'){\r\n              this.ruleForm.yuyueshijian = obj[o];\r\n              this.ro.yuyueshijian = true;\r\n              continue;\r\n            }\r\n            if(o=='yuyuebeizhu'){\r\n              this.ruleForm.yuyuebeizhu = obj[o];\r\n              this.ro.yuyuebeizhu = true;\r\n              continue;\r\n            }\r\n            if(o=='yonghuzhanghao'){\r\n              this.ruleForm.yonghuzhanghao = obj[o];\r\n              this.ro.yonghuzhanghao = true;\r\n              continue;\r\n            }\r\n            if(o=='xingming'){\r\n              this.ruleForm.xingming = obj[o];\r\n              this.ro.xingming = true;\r\n              continue;\r\n            }\r\n            if(o=='shouji'){\r\n              this.ruleForm.shouji = obj[o];\r\n              this.ro.shouji = true;\r\n              continue;\r\n            }\r\n            if(o=='fuwuz